# Example environment variables
# Copy this file to .env.local and fill in your values

# Database (if you plan to add backend functionality)
# DATABASE_URL="your_database_url_here"

# Email service (for form submissions)
# SMTP_HOST="your_smtp_host"
# SMTP_PORT=587
# SMTP_USER="your_email@example.com"
# SMTP_PASS="your_password"

# Analytics (optional)
# NEXT_PUBLIC_GOOGLE_ANALYTICS="GA_MEASUREMENT_ID"

# Social Media URLs (for the floating social bar)
NEXT_PUBLIC_LINKEDIN_URL="https://linkedin.com/company/profile"
NEXT_PUBLIC_INSTAGRAM_URL="https://instagram.com/profile"
NEXT_PUBLIC_TIKTOK_URL="https://tiktok.com/@profile"
NEXT_PUBLIC_FACEBOOK_URL="https://facebook.com/profile"
NEXT_PUBLIC_WHATSAPP_URL="https://wa.me/yourphonenumber"
